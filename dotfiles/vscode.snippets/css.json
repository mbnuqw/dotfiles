	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }
	"Debug Outline": {
		"prefix": "out",
		"body": "outline: 1px dashed rgba(0,0,0,.5); outline-offset: -1px;",
		"description": "Debug Outline"
	},
	"Debug Outline White": {
		"prefix": "outw",
		"body": "outline: 1px dashed rgba(255,255,255,.5); outline-offset: -1px;",
		"description": "Debug Outline (white)"
	},
	"Flex Center": {
		"prefix": "fc",
		"body": [
			"display: flex;",
			"justify-content: center;",
			"align-items: center;"
		],
		"description": "Flex center styles."
	}
}